<div class="about-me-container">
  <img class="about-me-image" src="<%- config.dstkd.person_info.nav_img_url %>" alt="<%- config.dstkd.person_info.nav_img_caption %>">
  <div class="about-me-info">
    <div class="about-me-intro"><%- config.dstkd.person_info.intro %></div>
    <div class="about-me-text"><%- config.dstkd.person_info.description %></div>
    <div class="soc-media-container">
      <a class="contact-envelope soc-media-link hvr-grow" title="Send me an e-mail"><i class="fas fa-envelope" aria-hidden="true"></i></a>
      <a class="soc-media-link hvr-grow" target="_blank" title="Follow me on Twitter" href="<%- config.dstkd.social_media_links.twitter %>"><i class="fab fa-twitter" aria-hidden="true"></i></a>
      <a class="soc-media-link hvr-grow" target="_blank" title="See my Github" href="<%- config.dstkd.social_media_links.github %>"><i class="fab fa-github" aria-hidden="true"></i></a>
      <a class="soc-media-link hvr-grow" target="_blank" title="Follow me on Medium" href="<%- config.dstkd.social_media_links.medium %>"><i class="fab fa-medium" aria-hidden="true"></i></a>
      <a class="soc-media-link hvr-grow" target="_blank" title="Find me on LinkedIn" href="<%- config.dstkd.social_media_links.linkedin %>"><i class="fab fa-linkedin" aria-hidden="true"></i></a>
    </div>
  </div>
</div>
